    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@smui/card": "^6.1.4",
    "@smui/checkbox": "^6.1.4",
    "@smui-extra/autocomplete": "6.2.0",
    "@smui/chips": "^6.0.0",
    "@smui/dialog": "^6.1.0",
    "@smui/drawer": "^6.0.0",
    "@smui/form-field": "^6.0.0",
    "@smui/icon-button": "^6.1.4",
    "@smui/list": "^6.1.0",
    "@smui/menu": "^6.2.0",
    "@smui/radio": "^6.1.0",

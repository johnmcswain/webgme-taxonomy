    "rollup-plugin-terser": "^7.0.0",
    "smui-theme": "^6.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-file-dropzone": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "ts-mocha": "^10.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"

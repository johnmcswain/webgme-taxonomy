// Common tsconfig info for webgme TypeScript components.
{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "composite": true,
    "tsBuildInfoFile": "tsconfig.tsbuildinfo"
  },
  "exclude": ["node_modules"]
}